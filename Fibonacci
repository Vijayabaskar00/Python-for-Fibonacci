# Function to generate Fibonacci sequence
def fibonacci(n):
    a, b = 0, 1
    sequence = []
    
    for _ in range(n):
        sequence.append(a)
        a, b = b, a + b
        
    return sequence

# Input: number of terms
n = int(input("Enter the number of Fibonacci terms you want: "))

# Output: Fibonacci sequence
fib_sequence = fibonacci(n)
print("Fibonacci sequence:")
print(fib_sequence)
